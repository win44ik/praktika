using System;
class Program
{
    static void Main()
    {
        Console.Write("Введите K: "); // Запрос количества простых чисел
        if (!int.TryParse(Console.ReadLine(), out int k) || k <= 0) // Проверка, является ли ввод положительным целым числом
        {
            Console.WriteLine("Ошибка: введите положительное целое число"); // Вывод ошибки при некорректном вводе
            return;
        }
        int count = 0; // Счетчик найденных простых чисел
        int num = 2; // Начальное число для проверки
        int lineCount = 0; // Счетчик чисел в текущей строке вывода

        while (count < k) // Цикл до нахождения K простых чисел
        {
            if (IsPrime(num)) // Проверка, является ли число простым
            {
                Console.Write(num + " "); // Вывод простого числа
                count++; // Увеличение счетчика простых чисел
                lineCount++; // Увеличение счетчика чисел в строке
                if (lineCount == 10) // Если выведено 10 чисел
                {
                    Console.WriteLine(); // Переход на новую строку
                    lineCount = 0; // Сброс счетчика чисел в строке
                }
            }
            num++; // Переход к следующему числу
        }
        Console.WriteLine(); // Переход на новую строку после вывода
    }
    static bool IsPrime(int n) // Метод для проверки, является ли число простым
    {
        if (n <= 1) return false; // Числа меньше или равные 1 не простые
        for (int i = 2; i <= Math.Sqrt(n); i++) // Проверка делителей от 2 до корня из n
        {
            if (n % i == 0) return false; // Если найден делитель, число не простое
        }
        return true; // Если делителей нет, число простое
    }
}
